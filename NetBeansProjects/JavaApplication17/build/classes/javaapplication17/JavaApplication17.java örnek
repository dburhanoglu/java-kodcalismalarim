package javaapplication17;

/**
 *
 * @author LENOVO
 */
public class JavaApplication17 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
      for(int i=1;i<100;i++) 
          if(i%3==0 &&i %7==0)
              System.out.println(""+i);//0 ile 100 arası 3 e ve 7 ye bölünebilen sayılar alttakindeyse 3 ve
      //7 aralarında asal olduğundan 21 in katı olan şeyleri alıyoruz
          for(int i=1;i<100;i++) 
          if(i%21==0)
              System.out.println(""+i);
          for(int i=1;i<4;i++)
              System.out.println(""+(21*i));
    }
    
}
